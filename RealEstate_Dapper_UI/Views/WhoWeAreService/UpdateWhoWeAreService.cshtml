
@model UpdateWhoWeAreServiceDtos
@{
    ViewData["Title"] = "UpdateWhoWeAreService";
    Layout = "~/Views/Admin/AdminLayout.cshtml";
}

<div class="col-sm-12 col-xl-6">
    <div class="bg-light rounded h-100 p-4">
        <h6 class="mb-4">Edit Service</h6>
        <form method="post">
            <div class="mb-3">
                <input type="hidden" class="form-control" asp-for="WhoWeAreServiceId">
                <br />
                <label for="exampleInputEmail1" class="form-label">Service</label>
                <input type="text" class="form-control" asp-for="ServiceName">
                <br />
                <label for="exampleInputEmail1" class="form-label">Status</label>
                <input type="text" class="form-control" asp-for="Status">
                <br />
                <button type="submit" class="btn btn-primary">Save</button>
            </div>
        </form>
    </div>
</div>

